# Google Gemini API Configuration
API_KEY=your_gemini_api_key_here

# Datadog Configuration
DATADOG_API_KEY=your_datadog_api_key_here
DATADOG_APP_KEY=your_datadog_app_key_here
DATADOG_SITE=datadoghq.com

# Datadog Agent Configuration
DD_AGENT_HOST=localhost
DD_DOGSTATSD_PORT=8125

# Datadog Service Configuration
DD_SERVICE=kairo-compass
DD_ENV=production
DD_VERSION=1.0.0

# Datadog Tags (comma-separated)
DD_TAGS=team:nav-engineering,service:kairo-api,project:kairo-compass

# Datadog APM Configuration (optional)
DD_TRACE_ENABLED=true
DD_TRACE_ANALYTICS_ENABLED=true
DD_LOGS_INJECTION=true
DD_PROFILING_ENABLED=false

# Application Configuration
NODE_ENV=production
PORT=3000

# Database Configuration (example)
DATABASE_URL=postgresql://user:password@localhost:5432/kairo
DB_POOL_MIN=2
DB_POOL_MAX=10

# Redis Configuration (example)
REDIS_URL=redis://localhost:6379

# External Services
WEATHER_API_KEY=your_weather_api_key
AIS_DATA_FEED_URL=https://ais-provider.com/feed

# GCP Configuration
GCP_PROJECT_ID=your_gcp_project_id
GCP_REGION=us-central1

# Feature Flags
ENABLE_DATADOG_MONITORING=true
ENABLE_SYNTHETIC_TESTS=true
ENABLE_APM_TRACING=true
